<h1>All Items</h1>

<ul>
<%=@items.each do |item|%>
    <label>Category:</label>
    <%=item.category%>

    <label>Sub-Category:</label>
    <%=item.sub_category%>

    <label>Stock:</label>
    <%=item.stock%>

    <label>Description:</label>
    <%=item.description%>

    <label>Size:</label>
    <%=item.size%>

    <label>Gender:</label>
    <%=item.gender%>
<%end%>
</ul>




